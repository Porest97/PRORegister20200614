                <label asp-for="PersonId2" class="control-label"></label>
                <select asp-for="PersonId2" class="form-control" asp-items="ViewBag.PersonId2"></select>
                <span asp-validation-for="PersonId2" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Description" class="control-label"></label>
                <input asp-for="Description" class="form-control" />
                <span asp-validation-for="Description" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="FEEntersSite" class="control-label"></label>
