        <dl class="row alert-success">
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.MtrlList)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.MtrlList.Description)
            </dd>
        </dl>
        <dl class="alert-success">
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.MtrlList.Item1)
            </dd>
            <dd class="col-sm-10">
