                <input asp-for="Logg" class="form-control" />
                <span asp-validation-for="Logg" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="IssueResolved" class="control-label"></label>
                <input asp-for="IssueResolved" class="form-control" />
