            </div>
            <div class="form-group">
                <label asp-for="IncidentStatusId" class="control-label"></label>
                <select asp-for="IncidentStatusId" class="form-control" asp-items="ViewBag.IncidentStatusId"></select>
                <span asp-validation-for="IncidentStatusId" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="IncidentTypeId" class="control-label"></label>
                <select asp-for="IncidentTypeId" class="form-control" asp-items="ViewBag.IncidentTypeId"></select>
                <span asp-validation-for="IncidentTypeId" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="IncidentNumber" class="control-label"></label>
                <input asp-for="IncidentNumber" class="form-control" />
                <span asp-validation-for="IncidentNumber" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Created" class="control-label"></label>
                <input asp-for="Created" class="form-control" />
                <span asp-validation-for="Created" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="PersonId" class="control-label"></label>
                <select asp-for="PersonId" class="form-control" asp-items="ViewBag.PersonId"></select>
                <span asp-validation-for="PersonId" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="SiteId" class="control-label"></label>
                <select asp-for="SiteId" class="form-control" asp-items="ViewBag.SiteId"></select>
                <span asp-validation-for="SiteId" class="text-danger"></span>
